    <record id="action_stock_transfer_charge_report" model="ir.actions.report">
      <field name="name">Cargos entre Locales</field>
      <field name="model">stock.picking</field>
      <field name="report_type">qweb-pdf</field>
      <!-- Ajusta <module_name> al valor real de tu módulo -->
