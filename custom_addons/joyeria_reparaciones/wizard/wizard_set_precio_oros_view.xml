<odoo>
  <record id="view_wizard_set_precio_oros_form" model="ir.ui.view">
    <field name="name">wizard.set.precio.oros.form</field>
    <field name="model">wizard.set.precio.oros</field>
    <field name="arch" type="xml">
      <form string="Precios Oro para Reporte">
        <group>
          <field name="precio_oro_amarillo"/>
          <field name="precio_oro_rosado"/>
        </group>
        <footer>
          <button name="action_print_report" type="object" string="Generar Reporte" class="btn-primary"/>
          <button string="Cancelar" special="cancel" class="btn-secondary"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
